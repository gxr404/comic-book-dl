{
  "name": "comic-book-dl",
  "version": "0.0.22",
  "description": "漫画下载器",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "bin": {
    "comic-book-dl": "bin/index.js"
  },
  "scripts": {
    "dev": "tsc -p tsconfig.build.json && (concurrently \"tsc -p tsconfig.build.json -w\" \"tsc-alias -p tsconfig.build.json -w\")",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "test": "run-s test:clean test:run",
    "test:run": "vitest run",
    "test:clean": "rimraf test/.temp",
    "eslintLog": "eslint . > eslint.log",
    "clean": "rimraf dist types",
    "np": "np",
    "release": "run-s clean build np",
    "postinstall": "patch-package"
  },
  "keywords": [
    "manga",
    "comic",
    "nodejs",
    "download",
    "comic-dl",
    "comic-book-dl",
    "comic-downloader",
    "cli"
  ],
  "author": "gxr404",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gxr404/comic-book-dl.git"
  },
  "dependencies": {
    "@inquirer/prompts": "^3.3.0",
    "cac": "^6.7.14",
    "cheerio": "^1.0.0-rc.12",
    "cli-progress": "3.12.0",
    "got": "^14.0.0",
    "log4js": "^6.9.1",
    "p-limit": "^5.0.0",
    "patch-package": "^8.0.0",
    "protobufjs": "^7.2.5",
    "rand-user-agent": "^1.0.109",
    "rimraf": "^5.0.5"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "concurrently": "^8.2.2",
    "np": "^9.2.0",
    "npm-run-all": "^4.1.5",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "overrides": {
    "@types/cacheable-request": "8.3.1"
  },
  "type": "module",
  "engines": {
    "node": ">=16.14.0"
  },
  "np": {
    "tests": true,
    "2fa": false
  }
}
